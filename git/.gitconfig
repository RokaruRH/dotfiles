[core]
	editor = zed
	ignorecase = false
	pager = delta

[interactive]
	diffFilter = delta --color-only

[delta]
	navigate = true
	line-numbers = true

[merge]
	conflictstyle = diff3

[diff]
	colorMoved = default

[init]
	defaultBranch = main

[fetch]
	prune = true

[pull]
	rebase = true
	default = current

[push]
	default = current
	autoSetupRemote = true

